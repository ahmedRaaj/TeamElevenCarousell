<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="carousell.teameleven.com.teamelevencarousell">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".InsertActivity" />
        <activity android:name=".EditActivity" />
        <activity android:name=".DeleteActivity" />
        <activity android:name=".ListByItems" />
        <activity android:name=".categorylistactivity" />
        <activity android:name=".ItemDetailActivity"></activity>
    </application>

</manifest>